{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start:front": "webpack serve",
    "start:dev": "concurrently -n front \"npm run start:front\"",
    "build:prod": "webpack --env mode=production apiUrl=https://skills-up-server.vercel.app/",
    "build:dev": "webpack --env mode=development",
    "build:analyze": "webpack --env mode=production analyze=true",
    "lint:ts": "npx tsc --noemit",
    "lint:code": "npx eslint",
    "lint:code:fix": "npx eslint --fix",
    "lint:scss": "npx stylelint '**/*.scss'",
    "lint:scss:fix": "npx stylelint --fix '**/*.scss'",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:sshot:run": "npx loki test --port 9000",
    "test:sshot:update": "npx loki approve",
    "test:sshot:ci": "npx loki --requireReference --reactUri file:./storybook-static",
    "storybook:dev": "storybook dev -p 9000 -c ./config/storybook --no-open",
    "storybook:build": "storybook build -c ./config/storybook",
    "report:sshot:gen": "npm run report:sshot:json && npm run report:sshot:html",
    "report:sshot:json": "npx ts-node scripts/gen-ui-json-report.ts",
    "report:sshot:html": "reg-cli --from .loki/report/report.json --report .loki/report/report.html",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "Oleksandr Brynzei",
  "license": "ISC",
